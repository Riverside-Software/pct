  <target name="test1">
  	<mkdir dir="build" />
  	<PCTCompile graphicalMode="false" destDir="build" dlcHome="${DLC}" failOnError="true">
      <fileset dir="src">
        <include name="test.p" />	
      </fileset>
    </PCTCompile>
  </target>
  <target name="test2">
