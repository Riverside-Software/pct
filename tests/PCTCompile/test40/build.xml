<?xml version="1.0"?>
<project name="PCTCompile-test40">
  <taskdef resource="PCT.properties" />

  <target name="test">
    <mkdir dir="build1" />
    <mkdir dir="build2" />

    <PCTCompile graphicalMode="false" destDir="build1" dlcHome="${DLC}">
      <fileset dir="src" />
      <Profiler enabled="${PROFILER}" coverage="true" outputDir="profiler" />
    </PCTCompile>

    <PCTCompile graphicalMode="false" destDir="build2" dlcHome="${DLC}" keepXref="true">
      <fileset dir="src" />
      <Profiler enabled="${PROFILER}" coverage="true" outputDir="profiler" />
    </PCTCompile>
  </target>

</project>
