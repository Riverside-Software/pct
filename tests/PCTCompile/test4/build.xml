<?xml version="1.0"?>
<project name="PCTCompile-test4">
  <taskdef resource="PCT.properties" />

  <target name="test">
    <mkdir dir="build" />
    <mkdir dir="build2" />
    <PCTCompile graphicalMode="false" destDir="build" dlcHome="${DLC}">
      <fileset dir="src">
        <include name="test.p" />
      </fileset>
      <Profiler enabled="${PROFILER}" coverage="true" outputDir="profiler" />
    </PCTCompile>
    <PCTCompile graphicalMode="false" destDir="build2" dlcHome="${DLC}" minSize="true">
      <fileset dir="src">
        <include name="test.p" />
      </fileset>
      <Profiler enabled="${PROFILER}" coverage="true" outputDir="profiler" />
    </PCTCompile>
  </target>

</project>
