<?xml version="1.0"?>
<project name="PCTCompile-test23">
  <taskdef resource="PCT.properties" />

  <target name="test1">
    <mkdir dir="build" />
    <PCTCompile graphicalMode="true" destDir="build" dlcHome="${DLC}">
      <fileset dir="src">
        <include name="*.p" />
      </fileset>
      <Profiler enabled="${PROFILER}" coverage="true" outputDir="profiler" />
    </PCTCompile>
  </target>

  <target name="test2">
    <PCTCompile graphicalMode="true" destDir="build" dlcHome="${DLC}" streamIO="true">
      <fileset dir="src">
        <include name="*.p" />
      </fileset>
      <Profiler enabled="${PROFILER}" coverage="true" outputDir="profiler" />
    </PCTCompile>
  </target>

</project>
