<?xml version="1.0"?>
<project name="PCTCompile-test59">
  <taskdef resource="PCT.properties" />

  <target name="test">
    <mkdir dir="build1" />
    <mkdir dir="build2" />
    <PCTCompile destDir="build1" dlcHome="${DLC}">
      <fileset dir="src" includes="*.p" />
      <propath location="src" />
      <Profiler enabled="${PROFILER}" coverage="true" outputDir="profiler" />
    </PCTCompile>
    <PCTCompile destDir="build2" dlcHome="${DLC}">
      <Option name="-checkdbe" />
      <fileset dir="src" includes="*.p" />
      <propath location="src" />
      <Profiler enabled="${PROFILER}" coverage="true" outputDir="profiler" />
    </PCTCompile>
  </target>
</project>
